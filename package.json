{
  "name": "@icylace/uy",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "Ron Martinez <ramartin.net@gmail.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.min.mjs",
  "unpkg": "./dist/index.umd.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "hyperapp": "^2.0.4",
    "redaxios": "^0.2.0",
    "remeda": "0.0.19",
    "shades": "^2.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "snowpack": "^1.7.0",
    "terser": "^4.6.11",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0"
  },
  "scripts": {
    "build:dev": "npm run clean && npm run transpile:full && npm run bundle",
    "build:dev:update": "npm run transpile:update && npm run bundle",
    "build:prod": "npm run build:dev && npm run min && npm run gzip",
    "bundle": "rollup --config",
    "bundle:watch": "rollup --config --watch",
    "check:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check:types": "tsc --noEmit --incremental false",
    "clean": "npm run clean:dist && npm run clean:transpiled",
    "clean:dist": "rm -fr ./dist && mkdir ./dist",
    "clean:transpiled": "rm -fr ./transpiled",
    "min": "npm run min:esm && npm run min:umd",
    "min:esm": "terser --ecma 6 --compress --mangle --module --output ./dist/index.min.js -- ./dist/index.js",
    "min:umd": "terser --ecma 6 --compress --mangle --output ./dist/index.umd.min.js -- ./dist/index.umd.js",
    "gzip": "npm run gzip:esm && npm run gzip:umd",
    "gzip:esm": "gzip --best --to-stdout ./dist/index.min.js > ./dist/index.min.js.gz",
    "gzip:umd": "gzip --best --to-stdout ./dist/index.umd.min.js > ./dist/index.umd.min.js.gz",
    "prepare": "snowpack",
    "transpile:full": "tsc --build --incremental false",
    "transpile:update": "tsc --build",
    "transpile:watch": "tsc --watch"
  }
}
