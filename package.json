{
  "name": "@icylace/uy",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "Ron Martinez <ramartin.net@gmail.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.min.mjs",
  "unpkg": "./dist/index.umd.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "hyperapp": "^2.0.4",
    "redaxios": "^0.2.0",
    "remeda": "0.0.19",
    "shades": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-typescript": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "rollup": "^2.6.1",
    "rollup-plugin-includepaths": "^0.2.3",
    "snowpack": "^1.7.0",
    "terser": "^4.6.11",
    "ts-jest": "^25.4.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0"
  },
  "scripts": {
    "build:dev": "npm run clean && npm run output:typescript:refresh && npm run bundle && npm run dist",
    "build:prod": "npm run clean && npm run output:typescript:full && npm run bundle && npm run min && npm run dist",
    "build:update": "npm run output:typescript:update && npm run bundle && npm run dist",

    "bundle": "npm run output:rollup && npm run output:babel",

    "check": "npm check:lint && npm check:types",
    "check:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check:types": "tsc --noEmit --incremental false",

    "clean": "rm -fr ./output && rm -fr ./dist && mkdir ./dist",

    "dist": "npm run dist:babel && npm run dist:typescript:types && npm run dist:terser && npm run dist:gzip",
    "dist:babel": "cp ./output/babel/* ./dist",
    "dist:gzip": "[ -d ./output/gzip ] && cp ./output/gzip/* ./dist ;:",
    "dist:typescript:types": "[ -f ./output/typescript/index.d.ts ] && cp ./output/typescript/index.d.ts ./dist ;:",
    "dist:terser": "[ -d ./output/terser ] && cp ./output/terser/* ./dist ;:",

    "min": "npm run output:terser && npm run output:gzip",

    "output:babel": "babel ./output/rollup --out-dir ./output/babel",

    "output:gzip": "[ ! -d ./output/gzip ] && mkdir ./output/gzip ; npm run output:gzip:esm && npm run output:gzip:umd",
    "output:gzip:esm": "gzip --best --to-stdout ./output/terser/index.min.js > ./output/gzip/index.min.js.gz",
    "output:gzip:umd": "gzip --best --to-stdout ./output/terser/index.umd.min.js > ./output/gzip/index.umd.min.js.gz",

    "output:rollup": "rollup --config",

    "output:terser": "[ ! -d ./output/terser ] && mkdir ./output/terser ; npm run output:terser:esm && npm run output:terser:umd",
    "output:terser:esm": "terser --ecma 6 --compress --mangle --module --output ./output/terser/index.min.js -- ./output/rollup/index.js",
    "output:terser:umd": "terser --ecma 6 --compress --mangle --output ./output/terser/index.umd.min.js -- ./output/rollup/index.umd.js",

    "output:typescript:full": "npm run output:typescript:refresh && npm run output:typescript:types",
    "output:typescript:refresh": "tsc --build --incremental false",
    "output:typescript:types": "tsc --declaration --emitDeclarationOnly --incremental false --module amd --outFile ./output/typescript/index.js",
    "output:typescript:update": "tsc --build",

    "prepare": "snowpack",

    "watch:typescript": "tsc --watch",
    "watch:rollup": "rollup --config --watch"
  }
}
