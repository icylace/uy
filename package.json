  },
  "bugs": "https://github.com/icylace/uy/issues",
  "author": {
    "name": "Ron Martinez",
    "email": "ramartin.net@gmail.com",
    "url": "https://ramartin.net/"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/uy.d.ts",
  "main": "./dist/uy.umd.js",
